  mm_menu_0229033131_0.addMenuItem("Hobbies","window.open('h.php', '_self');");
   mm_menu_0229033131_0.hideOnMouseOut=true;
   mm_menu_0229033131_0.menuBorder=1;
   mm_menu_0229033131_0.menuLiteBgColor='#669966';
   mm_menu_0229033131_0.menuBorderBgColor='#669966';
